{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kkd-learning-ADF"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"adv_works_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adv_works_storage'"
		},
		"onprimsqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'onprimsqlserver'",
			"defaultValue": "Integrated Security=True;Data Source=DE-IDK-PC776;Initial Catalog=AdventureWorksLT2022"
		},
		"AthleteHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Kuladeep561/tokyo-olympic-dataengineering-project/main/data/Athletes.csv"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kkdtokyoolympicdata2021.dfs.core.windows.net/"
		},
		"AzureDatabricks_advWorks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1130-132726-ywpht01m"
		},
		"AzureDatabricks_kevee_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1130-132726-ywpht01m"
		},
		"CoachesHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Kuladeep561/tokyo-olympic-dataengineering-project/main/data/Coaches.csv"
		},
		"EntriesGenderHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Kuladeep561/tokyo-olympic-dataengineering-project/main/data/EntriesGender.csv"
		},
		"MedalsHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Kuladeep561/tokyo-olympic-dataengineering-project/main/data/Medals.csv"
		},
		"TeamsHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Kuladeep561/tokyo-olympic-dataengineering-project/main/data/Teams.csv"
		},
		"adv_works_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kkdadventureworks.dfs.core.windows.net/"
		},
		"onprimsqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "idk\\kuk"
		},
		"sqlauth_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://tokyo-olympic-keys-vault.vault.azure.net/"
		},
		"tokyoHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Kuladeep561/tokyo-olympic-dataengineering-project/main/data/Teams.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kkd-learning-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"adv_works_storage_accountKey": {
						"value": "[parameters('adv_works_storage_accountKey')]"
					},
					"AthleteHTTP_properties_typeProperties_url": {
						"value": "[parameters('AthleteHTTP_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"CoachesHTTP_properties_typeProperties_url": {
						"value": "[parameters('CoachesHTTP_properties_typeProperties_url')]"
					},
					"EntriesGenderHTTP_properties_typeProperties_url": {
						"value": "[parameters('EntriesGenderHTTP_properties_typeProperties_url')]"
					},
					"MedalsHTTP_properties_typeProperties_url": {
						"value": "[parameters('MedalsHTTP_properties_typeProperties_url')]"
					},
					"TeamsHTTP_properties_typeProperties_url": {
						"value": "[parameters('TeamsHTTP_properties_typeProperties_url')]"
					},
					"adv_works_storage_properties_typeProperties_url": {
						"value": "[parameters('adv_works_storage_properties_typeProperties_url')]"
					},
					"sqlauth_properties_typeProperties_baseUrl": {
						"value": "[parameters('sqlauth_properties_typeProperties_baseUrl')]"
					},
					"tokyoHTTP_properties_typeProperties_url": {
						"value": "[parameters('tokyoHTTP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "kkd-learning-ADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"onprimsqlserver_connectionString": {
						"value": "[parameters('onprimsqlserver_connectionString')]"
					},
					"AzureDatabricks_advWorks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_advWorks_properties_typeProperties_existingClusterId')]"
					},
					"AzureDatabricks_kevee_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_kevee_properties_typeProperties_existingClusterId')]"
					},
					"onprimsqlserver_properties_typeProperties_userName": {
						"value": "[parameters('onprimsqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kkd-learning-ADF_ArmTemplate_0')]"
			]
		}
	]
}